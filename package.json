    "http-proxy": "^1.18.1",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.2",
    "react-popper": "^2.3.0",
    "react-transition-group": "^4.4.5",
    "rxjs": "^7.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
